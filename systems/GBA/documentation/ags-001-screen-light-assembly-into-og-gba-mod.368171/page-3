<script>
(function() {

  var user_name, api_key, twitch_widget;
  
  user_name = "gbatemptv";
  api_key = "c7fxl6i9icybv0qt2rvpavlxssirqc";
  twitch_widget = $("#twitch-widget");

  twitch_widget.attr("href","https://twitch.tv/" + user_name);

  $.getJSON('https://api.twitch.tv/kraken/streams/' + user_name + '?client_id=' + api_key + '&callback=?', function(data) {	
	  if (data.stream) {
		  twitch_widget.html("<span class='twitch_online' title='Playing " + data.stream.game + " for " + data.stream.viewers + " viewers'>Live</span>");
	  }
  });

})();
</script>
